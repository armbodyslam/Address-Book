{
    "parser": "babel-eslint",
    "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "allowImportExportEverywhere": true
    },
    "env": {
    "node": true,
    "commonjs": true,
    "browser": true,
    "es6": true
    },
    "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
    ],
    "rules": {
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off"
    },
    "globals": {
    "React": "writable"
    }
    }